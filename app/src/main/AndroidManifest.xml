        </activity>
        <activity android:name=".Ruang2.Booking2Activity" />
        <activity android:name=".InfoBookingActivity" />
        <activity android:name=".Ruang1.Booking1Activity" />
        <activity android:name=".Ruang2.Ruang2Activity" />
